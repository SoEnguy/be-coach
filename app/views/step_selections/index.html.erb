<h1>Listing step_selections</h1>

<table>
  <tr>
    <th>User</th>
    <th>Step</th>
    <th>Challenge</th>
    <th>Iscomplete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @step_selections.each do |step_selection| %>
  <tr>
    <td><%= step_selection.user_id %></td>
    <td><%= step_selection.step_id %></td>
    <td><%= step_selection.challenge_id %></td>
    <td><%= step_selection.isComplete %></td>
    <td><%= link_to 'Show', step_selection %></td>
    <td><%= link_to 'Edit', edit_step_selection_path(step_selection) %></td>
    <td><%= link_to 'Destroy', step_selection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Step selection', new_step_selection_path %>
